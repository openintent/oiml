{
  "name": "oiml-gin-auth",
  "version": "1.0.0",
  "description": "Gin authentication adapter template for OpenIntent",
  "category": "adapter",
  "framework": "gin",
  "adapter_type": "auth",
  "compatible_oiml_versions": ["0.1.x"],
  "compatible_package_versions": {
    "github.com/gin-gonic/gin": ["1.9.x", "1.10.x", "1.11.x"],
    "github.com/golang-jwt/jwt/v5": [">=5.0.0"],
    "golang.org/x/crypto": [">=0.0.0"]
  },
  "minimum_versions": {
    "go": "1.18"
  },
  "files": [
    {
      "path": "auth/middleware.go",
      "description": "JWT authentication middleware",
      "required": true
    },
    {
      "path": "auth/handlers.go",
      "description": "Authentication handlers (login, register, refresh)",
      "required": true
    }
  ],
  "dependencies": [
    "github.com/golang-jwt/jwt/v5",
    "golang.org/x/crypto/bcrypt"
  ]
}

